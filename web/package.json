{
  "type": "module",
  "name": "sed-web",
  "version": "0.1.0",
  "scripts": {
    "dev:frontend-internal": "vite --port 5003 --mode internal",
    "dev:frontend-external": "vite --port 5002",
    "dev:backend": "node src/api_server/apiServer.js",
    "dev": "concurrently 'npm:dev:frontend-internal' 'npm:dev:frontend-external' 'npm:dev:backend'",
    "dev:test": "export NODE_ENV='test' && npm run dev ",
    "build": "vite build --clearScreen=false",
    "lint": "vite lint",
    "test:unit": "export NODE_ENV='test' && vitest test/unit/ test/component test/views",
    "test:unit-cov-ci": "export NODE_ENV='test' && vitest run --coverage test/unit/ test/component test/views",
    "test:ui-e2e": "export NODE_ENV='test' && npx playwright test",
    "test:ui-e2e-ci": "export NODE_ENV='test' && npx playwright test --workers 2 --reporter=list",
    "test:ui-e2e-headed": "export NODE_ENV='test' && npx playwright test --workers=1 --headed",
    "test:ui-e2e-headed-single": "export NODE_ENV='test' && npx playwright test --workers=1 --headed -g '<test name here>'",
    "test:api-server": "vitest test/api_server/",
    "test:api-server-ci": "vitest run test/api_server/",
    "coverage": "export NODE_ENV='test' && vitest run --coverage test/unit/ test/component test/views",
    "sonar": "npm run coverage && npx sonar-scanner -Dsonar.login=sqp_68cad4cf1a12c258780055daa8d2e41b40392946"
  },
  "dependencies": {
    "@eds/vanilla": "^3.9.0",
    "axios": "1.4.0",
    "cors": "^2.8.5",
    "downloads-folder": "^1.0.3",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "is-in-subnet": "^4.0.1",
    "js-yaml": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "semver": "7.5.3",
    "swagger-ui-express": "^4.6.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.8",
    "concurrently": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^21.1.0",
    "less": "^4.1.1",
    "mock-fs": "^5.2.0",
    "prettier": "^2.4.1",
    "sonar-scanner": "^3.1.0",
    "tinyspy": "^1.0.2",
    "vite": "^4.0.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.28.5"
  }
}
